{
  "name": "ac-website",
  "private": true,
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint --fix src --report-unused-disable-directives",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,css,md}'",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "check-types": "tsc --noemit",
    "sanity": "eslint . --ext .js,.jsx,.ts,.tsx && prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "sanity:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "framer-motion": "^11.9.0",
    "heroicons-react": "^1.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.1",
    "react-use-measure": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.66",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
